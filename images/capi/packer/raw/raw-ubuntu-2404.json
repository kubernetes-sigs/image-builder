{
  "boot_command_prefix": "c<wait>linux /casper/vmlinuz ipv6.disable={{ user `boot_disable_ipv6` }} --- autoinstall ds='nocloud-net;s=http://{{ .HTTPIP }}:{{ .HTTPPort }}/24.04/'<enter><wait>initrd /casper/initrd<enter><wait>boot<enter>",
  "boot_disable_ipv6": "1",
  "boot_media_path": "/media/HTTP",
  "build_name": "ubuntu-{{user `distro_version` | replace_all \".\" \"\" }}",
  "build_target": "raw",
  "distro_arch": "amd64",
  "distro_name": "ubuntu",
  "distro_version": "24.04",
  "distro_version_slug": "2404",
  "guest_os_type": "ubuntu-64",
  "iso_checksum": "https://releases.ubuntu.com/noble/SHA256SUMS",
  "iso_checksum_type": "file",
  "iso_url": "https://releases.ubuntu.com/noble/ubuntu-24.04-live-server-amd64.iso",
  "os_display_name": "Ubuntu 24.04",
  "shutdown_command": "shutdown -P now",
  "unmount_iso": "true"
}
